@model List<Last4StuffViewModel>
<div class="row">


    @foreach (var item in Model)
    {
        <div class="col-lg-3 col-sm-6">
            <div class="card">
                <div class="card-body">
                    <div class="text-center">
                        <img src="@item.imageUrl" class="rounded-circle" width="110" height="110" alt="">
                        <h5 class="mt-3 mb-1">@item.nameSurname</h5>
                        <p class="m-0">@item.tittle</p>
                    </div>
                </div>
            </div>
        </div>
    }
</div>